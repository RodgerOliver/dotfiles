! .Xdefaults, older method and fallback, read everytime and Xlib program starts
! .Xresources, newer method, loads with xrdb
! ===== GENERAL =====
! disable scroll bar
URxvt*scrollBar: false
! font
URxvt*font: xft:monospace:size=12

! ===== COLORS =====
! text color
URxvt*foreground: #ffffff
! background
URxvt*background: #282a36
! cursor
URxvt*cursorColor: #afbfbf
! black
URxvt*color0:  #000000
URxvt*color8:  #555753
! red
URxvt*color1:  #cc0000
URxvt*color9:  #ef2929
! green
URxvt*color2:  #4e9a06
URxvt*color10: #8ae234
! yellow
URxvt*color3:  #c4a000
URxvt*color11: #fce94f
! blue
URxvt*color4:  #3465a4
URxvt*color12: #729fcf
! magenta
URxvt*color5:  #75507b
URxvt*color13: #ad7fa8
! cyan
URxvt*color6:  #06989a
URxvt*color14: #34e2e2
! white
URxvt*color7:  #d3d7cf
URxvt*color15: #eeeeec
