[alias]
	ck = checkout
	c = commit
	co = commit -m
	ca = commit -am
	st = status
	ps = push
	pl = pull
	b = branch
	ba = branch -avv
	fa = fetch --all
	s = show
	so = show --name-only
	d = diff
	l = log --decorate --oneline --graph
	ll = "log --all --graph --pretty=format:'%C(yellow)%h %C(green)%an %C(reset)%s %C(blue)%ar %C(auto)%d'"
	lo = "log --all --graph --pretty=format:'%C(yellow)%h %C(green)%an %C(reset)%s %C(blue)%ar %C(auto)%d%n'"
	pud = "!f() { \
		git push -f deploy HEAD:master; \
	}; f"
