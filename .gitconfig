[core]
	editor = vim
[diff]
	tool = vimdiff
	algorithm = patience
	indentHeuristic = true
[difftool]
	prompt = 0
[merge]
	tool = vimdiff
	conflictstyle = diff3
[alias]
	ck = checkout
	c = commit
	co = commit -m
	ca = commit -am
	st = status
	ps = push
	pl = pull
	b = branch
	ba = branch -avv
	fa = fetch --all
	s = show
	so = show --name-only
	d = diff
	ds = diff --cached
	l = log --decorate --oneline --graph
	lb = "log --pretty=format:'%C(yellow)%h %C(green)%an %C(reset)%s %C(blue)%ar %C(auto)%d' --graph master..HEAD"
	ll = "log --pretty=format:'%C(yellow)%h %C(green)%an %C(reset)%s %C(blue)%ar %C(auto)%d' --graph --all"
	lo = "log --pretty=format:'%C(yellow)%h %C(green)%an %C(reset)%s %C(blue)%ar %C(auto)%d' --graph"
	pud = "!f() { \
		git push -f deploy HEAD:master; \
	}; f"
